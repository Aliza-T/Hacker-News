@using HmwkAPI.Data.API
@model IEnumerable<HmwkAPI.Data.API.whatever>
<div>

        <table class="table table-striped table-bordered table-hover">
            <tr>
                <th> Title </th>
                <th> By </th>
                <th> Score </th>
            </tr>
            @foreach (var w in Model)
            {
                <tr>
                    <td>
                        <a target="_blank" href="@w.Url">@w.Title</a>
                    </td>
                    <td>@w.By</td>
                    <td>@w.Score</td>
            </tr>

            }

        </table>
    </div>


